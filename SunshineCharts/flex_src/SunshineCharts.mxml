<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" 
			   initialize="{init()}"  creationComplete="{complete()}"
			   xmlns:cofferChart="com.shine.cofferChart.*" width="100%" height="100%">
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			import com.shine.cofferChart.control.MainControl;
			
			private function init():void{
				MainControl.getInstance().put("type",String(this.parameters.type));
				MainControl.getInstance().put("dataUrl",String(this.parameters.dataUrl));
				MainControl.getInstance().put("cleanCache",String(this.parameters.cleanCache));
			}
			
			private function complete():void{
				
			}
		]]>
	</fx:Script>
	<cofferChart:CofferCharts id="cofferCharts" width="100%" height="100%" />
</s:Application>
