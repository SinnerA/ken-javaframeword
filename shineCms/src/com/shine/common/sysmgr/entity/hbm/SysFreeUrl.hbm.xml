<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">    
<hibernate-mapping>
	<class name="com.shine.common.sysmgr.entity.SysFreeUrl" table="SYS_FREE_URL" 
		dynamic-insert="false" dynamic-update="true">
		<id name="urlId" column="URLID" type="java.lang.Integer" length="8">
			<generator class="native" />
		</id>
		<property name="remark" column="REMARK" type="java.lang.String" length="50" not-null="false"/>
		<property name="uurl" column="UURL" type="java.lang.String" length="500" not-null="true"/>
		<property name="loginNeed" column="LOGINNEED" type="java.lang.Integer" length="1" not-null="true"/>
    </class>
</hibernate-mapping>