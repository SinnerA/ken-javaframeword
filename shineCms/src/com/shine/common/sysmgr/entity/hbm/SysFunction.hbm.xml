<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">    
<hibernate-mapping>
	<class name="com.shine.common.sysmgr.entity.SysFunction" table="SYS_FUNCTION" 
		dynamic-insert="false" dynamic-update="true">
		<id name="funId" column="FUNID" type="java.lang.Integer" length="8">
			<generator class="native" />
		</id>
		<property name="menuId" column="MENUID" type="java.lang.Integer" length="8" not-null="true"/>
		<property name="orderId" column="ORDERID" type="java.lang.Integer" length="4" not-null="true"/>
		<property name="funKey" column="FUNKEY" type="java.lang.String" length="50" not-null="true"/>
		<property name="funName" column="FUNNAME" type="java.lang.String" length="50" not-null="true"/>
		
		<!-- inverse="true" 与一般的一对多不太同，这里由SysFunction来维护关系 -->
		<set name="urls" cascade="all" inverse="true">
			<key column="FUNID" />
			<one-to-many class="com.shine.common.sysmgr.entity.SysFunctionUrl"/>
		</set>
		
		<!-- 为了初始化权限管理数据，设成非延迟加载了 lazy="false" fetch="join" -->
		<many-to-one name="sysMenu" column="MENUID" class="com.shine.common.sysmgr.entity.SysMenu" lazy="false" fetch="join" insert="false" update="false"></many-to-one>
    </class>
</hibernate-mapping>