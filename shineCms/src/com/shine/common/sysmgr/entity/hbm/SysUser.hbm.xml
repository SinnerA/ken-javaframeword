<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">    
<hibernate-mapping>
	<class name="com.shine.common.sysmgr.entity.SysUser" table="SYS_USER" 
		dynamic-insert="false" dynamic-update="true">
		<id name="userId" column="USERID" type="java.lang.Integer" length="11">
			<generator class="native" />
		</id>
		<property name="username" column="USERNAME" type="java.lang.String" length="50" not-null="true"/>
		<property name="password" column="PASSWORD" type="java.lang.String" length="128" not-null="true"/>
		<property name="name" column="NAME" type="java.lang.String" length="50" not-null="true"/>
		
		<property name="delflag" type="java.lang.Integer" not-null="false" insert="false" update="false">
			<column name="DELFLAG" sql-type="int(1)" />
		</property>
		<property name="deltime" column="DELTIME" type="java.util.Date" not-null="false" insert="false" update="false"/>
		
		<set name="roles" table="SYS_USER_ROLE" cascade="none" inverse="false" lazy="true">
			<key column="USERID" />
			<many-to-many column="ROLEID" class="com.shine.common.sysmgr.entity.SysRole" fetch="join" />
		</set>
    </class>
</hibernate-mapping>